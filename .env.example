# Example environment for EduAgent
# Copy this file as ".env" and fill in your own values

# Solana Devnet RPC URL
RPC_URL=https://api.devnet.solana.com

# Devnet keypair as JSON (private, do NOT share real one)
SECRET_KEY_JSON=[1,2,3,...]

# Optional: if you created a mock token or NFT mint on Devnet
KZTE_MINT=PASTE_MINT_ADDRESS_HERE
SCHOOL_WALLET=PASTE_SCHOOL_PUBLIC_KEY_HERE

# Backend port
PORT=8000
